{
  "name": "nda",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch app --exec node app/server.js --inspect=9223",
    "start": "yarn && npm run up && node app/server.js",
    "up": "knex-migrate up",
    "down": "knex-migrate down",
    "rollback": "knex-migrate rollback",
    "down-all": "knex-migrate down --to 0",
    "seed": "knex seed:run",
    "migration": "knex migrate:make",
    "remigrate": "npm run down-all && npm run up",
    "eslint": "./node_modules/.bin/eslint --ext .js --color ./ || exit 0",
    "eslint-fix": "./node_modules/.bin/eslint --ext .js --color ./ --fix",
    "pre-commit-eslint": "./node_modules/.bin/eslint --ext .js --color ./"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "ajv-keywords": "^3.4.1",
    "app-module-path": "^2.2.0",
    "aws-sdk": "^2.532.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "email-templates": "^6.0.2",
    "express": "^4.16.4",
    "http-errors": "^1.7.2",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.4",
    "knex-migrate": "^1.7.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "pg-promise": "^8.6.3",
    "pug": "^2.0.4",
    "squel": "^5.12.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit-eslint"
    }
  }
}
